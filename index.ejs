  <!-- Twitter -->
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:url" content="https://todayiread.vercel.app/">
  <meta property="twitter:title" content="TIR: Today I Read ｜ 社区驱动的高质量聚合阅读列表">
  <meta property="twitter:description"
    content="Today I Read, 社区驱动的高质量聚合阅读列表，提供高质量，及时，新潮，有趣的科技新闻， 编程技术，开源项目等， 打破 AI 推荐算法的信息茧房！">
  <meta property="twitter:image" content="https://todayiread.vercel.app/share-img.png">

  <!-- favicon -->
  <link rel="icon"
    href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 100 100%22><text y=%22.9em%22 font-size=%2290%22>📖</text></svg>">
  
  <!-- title -->
  <title>TIR: Today I Read ｜ 社区驱动的高质量聚合阅读列表</title>
  
  <!-- style -->
  <link rel="stylesheet" href="main.css">

  <!-- Counter.dev -->
  <script>if (!sessionStorage.getItem("_swa") && document.referrer.indexOf(location.protocol + "//" + location.host) !== 0) { fetch("https://counter.dev/track?" + new URLSearchParams({ referrer: document.referrer, screen: screen.width + "x" + screen.height, user: "Wenjian.jiang@foxmail.com", utcoffset: "8" })) }; sessionStorage.setItem("_swa", "1");</script>
</head>

<body>
  <div class="header">
    <div class="menu-left">
      <span>
        <mark>TIR</mark>.cool
      </span>
    </div>
    <div class="menu-right">
      <a class="menu-link" id="about-link" href="https://github.com/jwenjian/reading-list/discussions" target="_blank">🧑‍🤝‍🧑社区</a>
      <a class="menu-link" id="about-link" href="https://github.com/jwenjian/reading-list/wiki" target="_blank">💡关于</a>
    </div>
  </div>
  <img src="tir_logo.png" alt="TIR Logo" style="height: 10vh;">
  <h3>社区驱动的高质量聚合阅读列表</h3>
  <h5 id="current-date"></h5>
  <div class="wrapper">
    <div class="data-list">
      <ul class="list">
        <% today.forEach(function(item_in_today) { %>
         <li><a href="<%= item_in_today.body %>" target="_blank"><%= item_in_today.title %></a></li> 
        <% }) %>
      </ul>
      <hr class="split-line">
      <h5>近期</h5>
      <ul class="recent-list">
        <% recent.forEach(function(item_in_recent) { %>
          <li><a href="<%= item_in_recent.html_url %>" target="_blank"><%= item_in_recent.title %></a></li> 
         <% }) %>
      </ul>
    </div>
  </div>
  <div class="footer">
    <a href="https://github.com/jwenjian/reading-list/issues" target="_blank">归档</a>
    <p>上次更新于：<span id="update_at"><%= update_at %></span></p>
    <p>Made with 💖 by <a href="https://github.com/jwenjian" target="_blank">@jwenjian</a> </p>
  </div>
</body>
<script src="page.js"></script>
</html>
